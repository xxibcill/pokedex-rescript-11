{
  "name": "pokedex-rescript-11",
  "namespace": false,
  "jsx": {
    "version": 4
  },
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "@glennsl/rescript-fetch",
    "@glennsl/rescript-json-combinators"
  ],
  "bsc-flags": ["-open RescriptCore", "-open JsonCombinators"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".res.mjs",
  "gentypeconfig": {
    "language": "untyped",
    "shims": []
  }
}
